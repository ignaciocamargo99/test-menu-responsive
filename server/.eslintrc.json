{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "no-unused-vars": 1,
        "indent": 0,
        "brace-style": 0,
        "@typescript-eslint/explicit-function-return-type": [
            "error"
        ],
        "no-undef":"warn"
    }
}